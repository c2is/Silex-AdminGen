{% extends "@AdminGen/layout.html.twig" %}

{% block content %}
<div id="component_{{ name }}" class="component">
    <div class="component-header">
        <span class="title">{{ theme_title|default('') }}</span>
        <div class="actions">
            {{ theme_button|default('') }}
        </div>
    </div>
    <div class="component-content">
        <div class="container">
        {{ theme_content|default('') }}
        </div>
    </div>
    <div class="component-footer">
        {{ theme_footer|default('') }}
    </div>
</div>
{% endblock %}
